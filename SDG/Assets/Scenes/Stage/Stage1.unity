    narration: []
    script: 
  - name: Janice
    longText: Wow! Your effort is just amazing.
    img: {fileID: 2800000, guid: ecad5b8a47d7c884e9bd530449ae3e50, type: 3}
    choices: []
    choiceMode: 0
    links: []
    next: 30
