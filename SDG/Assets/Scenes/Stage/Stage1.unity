      shortText: 
      next: 0
      editor: 0
    align: 0
    mode: 0
    narration: []
    script: 
  - name: Janice
    longText: "I\u2019m not complaining about this, and I\u2019m not annoyed. But
      I want to know why people here, even a young child like him, is starving."
    img: {fileID: 2800000, guid: 9ca753577d356844a9b49d743e48a1aa, type: 3}
    choices: []
    choiceMode: 0
    links: []
    next: 22
