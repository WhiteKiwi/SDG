      next: 0
      editor: 0
    align: 1
    mode: 0
    narration: []
    script: 
  - name: Aziza
    longText: "But the situation is not getting better. Sometimes I can\u2019t even
      eat 1 meal in a day. But I\u2019m much better than other villagers, since I
      get much better pay compared to others, thanks to my English skills."
    img: {fileID: 2800000, guid: 2aa74f103d9b8a6419d6ee2d5d737d42, type: 3}
    choices: []
    choiceMode: 0
    links: []
