    narration: []
    script: 
  - name: Janice
    longText: What do you mean?
    img: {fileID: 2800000, guid: 9ca753577d356844a9b49d743e48a1aa, type: 3}
    choices: []
    choiceMode: 0
    links: []
    next: 26
    editor: 0
    pos: 1
    passwords: []
    incorrect: 0
    exit:
      shortText: 
      next: 0
      editor: 0
    align: 1
    mode: 0
    narration: []
    script: 
  - name: Aziza
    longText: "Actually, I\u2019m the only one who finished elementary school education
      in our village. So I decided to help my village with skill in speaking English
      fluently."
    img: {fileID: 2800000, guid: 2aa74f103d9b8a6419d6ee2d5d737d42, type: 3}
